{
  "AllowedHosts": "*",
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "BattlenetApi": {
    "clientId": "{BattlenetApiClientId}",
    "clientSecret": "{BattlenetApiClientSecret}"
  },
  "TwitchApi": {
    "clientId": "{TwitchApiClientId}",
    "clientSecret": "{TwitchApiClientSecret}"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}